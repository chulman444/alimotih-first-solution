{
  "name": "Alimotih first solution",
  "description": "Alimotih",
  "version": "0.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Alimotih",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["activeTab", "tabs", "storage"],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}